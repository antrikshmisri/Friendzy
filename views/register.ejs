<%- include('partials/head') %>
    <body>

        <div class="container-fluid main-container">
            <div class="row justify-content-center mx-0">
              <div class="col-lg-12 col-sm-12 signin-container">
                <h1 class="title">REGISTER</h1>
                  <div class="container user-inputs">
                    <div class="row">
                      <form>
                      <div class="col-lg-12">
                        <input id="userEmail"  oninput="checkEmail('userEmail' , 'userEmailError')" type="text" required placeholder="Email" />
                        <small id="userEmailError" class="form-text text-danger">This Field must be filled.</small>
                      </div>
                      <div class="col-lg-12">
                        <input id="userPassword"  oninput="checkPass('userPassword' , 'userPasswordError')" type="password" required placeholder="Password" />
                        <small id="userPasswordError" class="form-text text-danger">Your password must be uppercase, lowercase and more than 10 characters.</small>
                      </div>
                      <div class="col-lg-12 my-2 register">
                        <button type="submit" id="register" class="btn disabled">Register</button>
                      </div>
                      </form>
                    </div>
                  </div>
              </div>
            </div>
          </div>
        <script src="" async defer></script>
        <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>
    </body>
</html>